//FACTORIAL
using System;
namespace hi;
class Program{
  static void Main(string[] args){
    Console.Write("Enter Num(Factorial):");
    int n=Convert.ToInt32(Console.ReadLine());
    int fact=1;
    for(int i=1;i<=n;i++){
       fact=fact*i;
    }
    Console.WriteLine(" Factorial of {0} is {1}",n,fact);
  }
}
